{
  "plugins": [
    ["i18next-extract", {
      "outputPath": "./src/locale/{{locale}}/{{ns}}.json",
      "keyAsDefaultValue": true,
      "useI18nextDefaultValue": true,
      "locales": ["en", "se"],
      "discardOldKeys": true
    }],
    ["@babel/plugin-transform-react-jsx"],
    ["@babel/plugin-proposal-class-properties"],
    ["babel-plugin-styled-components"]
  ]
}

